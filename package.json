    "@types/node": "^12.7.2",
    "codelyzer": "^5.1.0",
    "cspell": "^4.0.28",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.5.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
