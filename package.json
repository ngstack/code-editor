    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.5",
    "codelyzer": "^5.1.2",
    "cspell": "^4.0.63",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.0.7",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.3",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.1",
    "tslint": "~6.1.2",
    "typescript": "3.8.3"
