{
  "name": "@ngstack/core",
  "description": "An open source toolkit for enterprise Angular applications.",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Denys Vuika <denys.vuika@gmail.com>",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "ng test code-editor --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "spellcheck": "cspell 'src/**/*.ts' 'e2e/**/*.ts' 'projects/**/*.ts'",
    "build:lib": "ng build code-editor && cp -R projects/code-editor/src/lib/workers dist/@ngstack/code-editor",
    "build:prod": "ng build code-editor --prod && cp -R projects/code-editor/src/lib/workers dist/@ngstack/code-editor",
    "test:ci": "ng test code-editor --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.11",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.11",
    "@angular/compiler": "^10.0.11",
    "@angular/core": "^10.0.11",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.0.11",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.11",
    "@angular/platform-browser-dynamic": "^10.0.11",
    "@angular/router": "^10.0.11",
    "monaco-editor": "^0.20.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular-devkit/build-ng-packagr": "~0.1000.6",
    "@angular/cli": "^10.0.7",
    "@angular/compiler-cli": "^10.0.11",
    "@angular/language-service": "^10.0.11",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.27",
    "codelyzer": "^5.1.2",
    "cspell": "^4.0.63",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.4",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.2",
    "typescript": "3.9.7"
  }
}
